FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      extrudeProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

constructFrom   mesh;
sourceCase      "<case>";
sourcePatches   (outlet);

flipNormals     false;

extrudeModel    linearDirection;

nLayers             70;
expansionRatio      1.0;

linearDirectionCoeffs
{
    axisPt          (0 0 0);
    direction       (1 0 0);
    thickness       7;
}

mergeFaces      false;

